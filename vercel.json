{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" },
    { "src": "styles/**", "use": "@vercel/static" },
    { "src": "pdf/**", "use": "@vercel/static" },
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "favicon.ico", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}